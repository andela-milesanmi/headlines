{
  "name": "mai-headlines",
  "version": "1.0.0",
  "description": "A simple News Feed Application that displays published articles on a range of news sources and blogs from the NewsAPI.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "cross-env NODE_ENV=test nyc mocha --compilers js:babel-core/register ./app/tests/**/*.test.jsx",
    "coverage": "nyc report --reporter=text-lcov | coveralls && npm run test",
    "codeclimate": "CODECLIMATE_REPO_TOKEN=process.env.CODECLIMATE_REPO_TOKEN codeclimate-test-reporter < lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-milesanmi/mai-headlines.git"
  },
  "keywords": [
    "headlines",
    "andela",
    "checkpoint",
    "news",
    "api",
    "mai headlines"
  ],
  "author": "Maranatha A. Ilesanmi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-milesanmi/mai-headlines/issues"
  },
  "homepage": "https://github.com/andela-milesanmi/mai-headlines#readme",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-core": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.4.5",
    "express": "^4.15.2",
    "flux": "^3.1.2",
    "google-auth-library": "^0.10.0",
    "history": "^4.6.1",
    "js-cookie": "^2.1.4",
    "jsdom": "^10.0.0",
    "jsdom-global": "^2.1.1",
    "key-mirror": "^1.0.1",
    "mocha": "^3.3.0",
    "object-assign": "^4.1.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-google-login": "^2.8.9",
    "react-input-autosize": "^1.1.3",
    "react-router": "^3.0.2",
    "react-select": "^1.0.0-rc.3",
    "react-share": "^1.13.2",
    "sinon": "^2.2.0",
    "superagent": "^3.5.2"
  },
  "devDependencies": {
    "babel-loader": "^6.4.1",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai-http": "^3.0.0",
    "coveralls": "^2.13.1",
    "create-react-class": "^15.5.2",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "expect": "^1.20.2",
    "expect-jsx": "^3.0.0",
    "istanbul": "^0.4.5",
    "jsdom": "^10.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "node-sass": "^4.5.2",
    "nyc": "^10.3.2",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 70,
    "statements": 70,
    "functions": 70,
    "branches": 70,
    "require": [
      "babel-register"
    ],
    "exclude": [
      "**/*.test.jsx",
      "public"
    ],
    "extension": [
      ".jsx",
      ".es6"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
